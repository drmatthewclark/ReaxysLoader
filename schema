--
-- Name: reaxys_conditions; Type: VIEW; Schema: reaxys; Owner: -
--

CREATE VIEW reaxys.reaxys_conditions AS
 SELECT rdfile.rx_id,
    reaction.reaction_id,
    NULL::bigint AS stage_id,
    variation.variation_id,
    conditions.condition_id
   FROM reaxys.rdfile,
    reaxys.reaction,
    reaxys.variation,
    reaxys.conditions
  WHERE ((variation.variation_id = ANY (reaction.variations)) AND (conditions.condition_id = ANY (variation.conditions)) AND (reaction.reaxys_reaction_id = rdfile.rx_id))
UNION
 SELECT rdfile.rx_id,
    reaction.reaction_id,
    stages.stage_id,
    variation.variation_id,
    conditions.condition_id
   FROM reaxys.rdfile,
    reaxys.reaction,
    reaxys.variation,
    reaxys.stages,
    reaxys.conditions
  WHERE ((variation.variation_id = ANY (reaction.variations)) AND (stages.stage_id = ANY (variation.stages)) AND (conditions.condition_id = ANY (stages.conditions)) AND (reaction.reaxys_reaction_id = rdfile.rx_id));


